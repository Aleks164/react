{
  "name": "test",
  "version": "1.0.0",
  "description": "react start ptoject",
  "main": "./src/index.tsx",
  "scripts": {
    "build": "webpack build --node-env production",
    "dev": "webpack serve --open --node-env development",
    "test": "jest --coverage",
    "lint": "prettier --check . && eslint . --ext .js,.tsx && tsc --noEmit && stylelint \"**/*.css\"",
    "lint:fix": "prettier --write . && eslint . --fix --ext .js,.tsx && stylelint \"**/*.css\" --fix",
    "tsc": "npx tsc --noEmit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "loki:update": "npm run build-storybook && loki update --requireReference --reactUri file:./storybook-static",
    "loki:approve": "loki approve",
    "loki:test": "npm run build-storybook && loki test --requireReference --reactUri file:./storybook-static",
    "predeploy": "npm run build-storybook",
    "deploy": "gh-pages -d storybook-static",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.7",
    "gh-pages": "^3.2.3",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "loki": "^0.28.1",
    "mini-css-extract-plugin": "^2.5.3",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "ts-loader": "^9.2.7",
    "typescript": "^4.5.5",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "lint-staged": {
    "*.{js,tsx}": [
      "prettier --write",
      "eslint --cache --fix --ext .js,.tsx"
    ],
    "*.{css,md,yml}": "prettier --write",
    "*.css": "stylelint --fix"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "stylelint-config-standard": "^25.0.0"
  }
}
